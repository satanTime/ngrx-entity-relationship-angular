{
  "name": "ngrx-entity-relationship-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "11.2.2",
    "@angular/compiler": "11.2.2",
    "@angular/core": "11.2.2",
    "@angular/platform-browser": "11.2.2",
    "@angular/platform-browser-dynamic": "11.2.2",
    "@ngrx/data": "11.0.1",
    "@ngrx/effects": "11.0.1",
    "@ngrx/entity": "11.0.1",
    "@ngrx/store": "11.0.1",
    "core-js": "3.9.0",
    "ngrx-entity-relationship": "1.5.0",
    "rxjs": "6.6.3",
    "tslib": "2.0.3",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1102.1",
    "@angular/cli": "11.2.1",
    "@angular/compiler-cli": "11.2.2",
    "@types/node": "12.20.4",
    "husky": "5.1.1",
    "prettier": "2.2.1",
    "ts-node": "9.1.1",
    "typescript": "4.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./node_modules/.bin/prettier -u -w ."
    }
  },
  "renovate": {
    "extends": [
      "config:base",
      ":semanticCommitTypeAll(chore)"
    ],
    "prConcurrentLimit": 0,
    "prHourlyLimit": 0,
    "rangeStrategy": "pin",
    "rebaseWhen": "conflicted",
    "separateMajorMinor": true,
    "separateMinorPatch": true,
    "separateMultipleMajor": true,
    "ignoreDeprecated": false,
    "semanticCommitScope": "{{#if baseDir}}{{baseDir}}{{else}}deps{{/if}}",
    "additionalBranchPrefix": "{{#if baseDir}}{{baseDir}}/{{else}}root/{{/if}}",
    "reviewers": [
      "satanTime"
    ]
  }
}
