{
  "name": "ngrx-entity-relationship-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "12.2.2",
    "@angular/compiler": "12.2.2",
    "@angular/core": "12.2.2",
    "@angular/platform-browser": "12.2.2",
    "@angular/platform-browser-dynamic": "12.2.2",
    "@ngrx/data": "12.4.0",
    "@ngrx/effects": "12.4.0",
    "@ngrx/entity": "12.4.0",
    "@ngrx/store": "12.4.0",
    "core-js": "3.16.2",
    "ngrx-entity-relationship": "1.7.0",
    "rxjs": "6.6.7",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.1",
    "@angular/cli": "12.2.1",
    "@angular/compiler-cli": "12.2.2",
    "@types/node": "14.17.10",
    "husky": "7.0.1",
    "prettier": "2.3.2",
    "ts-node": "10.2.0",
    "typescript": "4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./node_modules/.bin/prettier -u -w ."
    }
  },
  "renovate": {
    "extends": [
      "config:base",
      ":semanticCommitTypeAll(chore)"
    ],
    "prConcurrentLimit": 0,
    "prHourlyLimit": 0,
    "rangeStrategy": "pin",
    "rebaseWhen": "conflicted",
    "separateMajorMinor": true,
    "separateMinorPatch": true,
    "separateMultipleMajor": true,
    "ignoreDeprecated": false,
    "semanticCommitScope": "{{#if baseDir}}{{baseDir}}{{else}}deps{{/if}}",
    "additionalBranchPrefix": "{{#if baseDir}}{{baseDir}}/{{else}}root/{{/if}}",
    "reviewers": [
      "satanTime"
    ]
  }
}
